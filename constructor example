import java.util.*;
class Test
{
    void show()
    {
        System.out.println("show method");
    }
    Test()
    {
        System.out.println("constructor");
    }
    public static void main(String args[])
    {
        Test t1=new Test();
        Test t2=new Test();
        t1.show();
    }
}
