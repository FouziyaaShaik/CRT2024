import java.util.*;
class Father 
{
    void surName()
    {
        System.out.println("k");
    }
    void designation()
    {
        System.out.println("clerk");
    }
}
class Child extends Father 
{
    void getName()
    {
        System.out.println("satish");
    }
    void designation()
    {
        System.out.println("java programmer");
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Child c1=new Child();
        c1.getName();
        c1.surName();
        c1.designation();
    }
}

satish
k
java programmer

=== Code Execution Successful ===

import java.util.*;
class Father 
{
    void surName()
    {
        System.out.println("Shaik");
    }
    void designation()
    {
        System.out.println("clerk");
    }
}
class Child extends Father 
{
    void getName()
    {
        System.out.println("Fouziya");
    }
    void designation()
    {
        System.out.println("C programmer");
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Child c1=new Child();
        c1.getName();
        c1.surName();
        c1.designation();
    }
