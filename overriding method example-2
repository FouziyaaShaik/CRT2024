import java.util.*;
class Father 
{
    void surName()
    {
        System.out.println("Shaik");
    }
    void designation()
    {
        System.out.println("clerk");
    }
}
class Child extends Father 
{
    void getName()
    {
        System.out.println("Fouziya");
    }
    void designation()
    {
        System.out.println("C programmer");
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Child c1=new Child();
        Father f1=new Father();
        c1.getName();
        c1.surName();
        c1.designation();
        f1.designation();
    }
}
Fouziya
Shaik
C programmer
clerk

=== Code Execution Successful ===
