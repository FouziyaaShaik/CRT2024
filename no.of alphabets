#include <stdio.h>
#include<string.h>
int main() {
    char str[100];
    int i,n,j,r;
    scanf("%[^\n]s",str);
    n=strlen(str); 
    for(i=1;i<n;i=i+2)
    {
        r=(int)str[i]-48;
       for(j=0;j<r;j++)
       {
           printf("%c",str[i-1]);
       }
    }
    return 0;
}

