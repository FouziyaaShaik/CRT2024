import java.util.*;
class Sample
{
    int a=10;
    static int b=20;
    void show()
    {
        int c=30;
        System.out.println("a="+a);
        System.out.println("b="+b);
        System.out.println("c="+c);
    }
}
public class main
{
    public static void main(String[] args) 
    {
        Sample s1=new Sample();
        s1.show();
    }
}

a=10
b=20
c=30

=== Code Execution Successful ===
