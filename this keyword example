import java.util.*;
class Test
{
    int a=10, b=20;
    void show(int a, int b)
    {
        System.out.println("a+b");
        System.out.println(this.a+this.b);
    }
}
public class Main
{
    public static void main(String args[])
    {
        Test t1=new Test();
        t1.show(100,200);
    }
}

a+b
30

=== Code Execution Successful ===
