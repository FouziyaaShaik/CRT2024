#include <stdio.h>
void swap(int *x,int *y)
{
    int t;
    t=*x;
    *x=*y;
    *y=t;
}
int main() {
    int a,b;
     printf("enter two values");
     scanf("%d%d",&a,&b);
     printf("values before swapping a=%d b=%d\n",a,b);
     swap(&a,&b);
     printf("values after swapping a=%d b=%d\n",a,b);
    return 0;
}

enter two values 66
99
values before swapping a=6 b=99
values after swapping a=99 b=6


=== Code Execution Successful ===
